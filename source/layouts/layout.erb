<!doctype html>

<%= partial "partials/html" %>

  <head>
    <meta charset="utf-8">

    <title><%= page_title %></title>

    <%= partial "partials/meta" %>
    <%= partial "partials/css" %>
    <%= partial "partials/head/js" %>
    <%= partial "partials/link" %>
    <script src="http://d3js.org/d3.v3.min.js"></script>
    <script src="http://dimplejs.org/dist/dimple.v1.1.5.min.js"></script>
  </head>
  
  <body class="page <%= page_classes %>">

    <header class="header" role="banner">
    </header>

    <main class="main" role="main">
      <%= yield %>
    </main>

    <footer class="footer">
    </footer>

    <%= partial "partials/foot/js" %>
  </body>
</html>
